# /proc/bandinfo - Radio
genfscon proc /bandinfo            u:object_r:proc_bandinfo:s0

# /proc/AllHWLis/* - Gestures
genfscon proc /AllHWList/tp_double_tap   u:object_r:proc_fih:s0

# Sensors
genfscon proc /sensordata       u:object_r:proc_fih:s0

# Sysfs
genfscon sysfs /sys/devices/soc/soc:qcom,ipa_fws@1e08000/subsys0/name                                                                       u:object_r:sysfs_ssr:s0
genfscon sysfs /sys/devices/soc/17300000.qcom,lpass/subsys4/state                                                                           u:object_r:sysfs_ssr:s0
