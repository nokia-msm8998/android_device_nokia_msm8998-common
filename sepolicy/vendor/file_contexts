# Audio
/(vendor|system/vendor)/bin/init\.fih\.smartamp_init\.sh	u:object_r:smartamp_init_exec:s0

# Firmware
/bt_firmware(/.*)?                  u:object_r:bt_firmware_file:s0
/firmware(/.*)?                     u:object_r:firmware_file:s0
/persist(/.*)?                      u:object_r:mnt_vendor_file:s0

# Goodix Fingerprint
/dev/goodix_fp		                    u:object_r:gx_fpd_device:s0
/data/system/fingerprint(/.*)?              u:object_r:gx_fpd_data_file:s0
/(vendor|system/vendor)/bin/gx_fpd          u:object_r:gx_fpd_exec:s0

# HALs
/(vendor|system/vendor)/bin/hw/android\.hardware\.power-service\.nokia-libperfmgr                      u:object_r:hal_power_default_exec:s0
/(vendor|system/vendor)/bin/hw/android\.hardware\.power\.stats@1\.0-service\.mock                      u:object_r:hal_power_stats_default_exec:s0

# Hexagon DSP-side executable needed for Halide operation
# This is labeled as public_adsprpcd_file as it needs to be read by apps
# (e.g. Google Camera App)
/mnt/vendor/dsp/fastrpc_shell_3                        u:object_r:public_adsprpcd_file:s0

# Industrial I/O nodes
/sys/bus/iio/devices(/.*)?                              u:object_r:sysfs_iio:s0
/sys/devices/platform/soc/.*/iio:device[0-9](/.*)?      u:object_r:sysfs_iio:s0
